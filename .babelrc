{
  "plugins": [
    "add-module-exports"
  ],
  "presets": [
    "react",
    "es2015",
    "es2016",
    "stage-1"
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", { "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
        }]}]
      ]
    }
  }
}
